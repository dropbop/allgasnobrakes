{
  "version": 2,
  "builds": [
    { "src": "app.py", "use": "@vercel/python" }
  ],
  "routes": [
    {
      "src": "/static/js/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=0, must-revalidate"
      },
      "continue": true
    },
    { 
      "src": "/(.*)", 
      "dest": "/app.py" 
    }
  ]
}
